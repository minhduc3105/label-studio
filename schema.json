  /api/projects/:
    get:
      operationId: api_projects_list
      description: "\n    Return a list of the projects that you've created.\n\n \
        \   To perform most tasks with the Label Studio API, you must specify the\
        \ project ID, sometimes referred to as the `pk`.\n    To retrieve a list of\
        \ your Label Studio projects, update the following command to match your own\
        \ environment.\n    Replace the domain name, port, and authorization token,\
        \ then run the following from the command line:\n    ```bash\n    curl -X\
        \ GET https://localhost:8080/api/projects/ -H 'Authorization: Token abc123'\n\
        \    ```\n    "
      summary: List your projects
      parameters:
      - in: query
        name: filter
        schema:
          type: string
          default: all
        description: Filter projects by pinned status. Use 'pinned_only' to return
          only pinned projects, 'exclude_pinned' to return only non-pinned projects,
          or 'all' to return all projects.
      - in: query
        name: ids
        schema:
          type: string
        description: Filter id by in list
      - in: query
        name: include
        schema:
          type: string
        description: 'Comma-separated list of count fields to include in the response
          to optimize performance. Available fields: task_number, finished_task_number,
          total_predictions_number, total_annotations_number, num_tasks_with_annotations,
          useful_annotation_number, ground_truth_number, skipped_annotations_number.
          If not specified, all count fields are included.'
      - name: ordering
        required: false
        in: query
        description: Which field to use when ordering the results.
        schema:
          type: string
      - name: page
        required: false
        in: query
        description: A page number within the paginated result set.
        schema:
          type: integer
      - name: page_size
        required: false
        in: query
        description: Number of results to return per page.
        schema:
          type: integer
      - in: query
        name: search
        schema:
          type: string
        description: Search term for project title and description
      - in: query
        name: title
        schema:
          type: string
        description: Filter title by contains (case-insensitive)
